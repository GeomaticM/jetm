<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:jetm="http://jetm.void.fm/schema/jetm-spring-config-1.2"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
                           http://jetm.void.fm/schema/jetm-spring-config-1.2 http://jetm.void.fm/schema/jetm-spring-config-1.2.xsd">


  <jetm:monitor id="bla" type="nested" timer="etm.core.timer.Java15NanoTimer">
    <jetm:feature>
      <jetm:buffer type="threshold">
        <jetm:property name="threshold">1000</jetm:property>
      </jetm:buffer>
      <jetm:persistence enabled="true">
        <jetm:property name="key">value</jetm:property>
      </jetm:persistence>

      <jetm:raw-data-log type="log4j">
        <jetm:property name="key">blabla</jetm:property>
      </jetm:raw-data-log>
    </jetm:feature>
  </jetm:monitor>

</beans>