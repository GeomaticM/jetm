<rrd_graph_def>
  <filename>${imagefile}</filename>
  <span>
    <start>${intervalstart}</start>
    <end>${intervalend}</end>
  </span>
  <options>
    <anti_aliasing>true</anti_aliasing>
    <title>${imagetitle}</title>
    <width>600</width>
    <height>200</height>
    <units_exponent>0</units_exponent>
    <!--<units_length>0</units_length>-->
    <rigid>true</rigid>
    <logarithmic>${logarithmic}</logarithmic>
  </options>

  <datasources>
    <def>
      <name>min-avg</name>
      <rrd>${rrdfile}</rrd>
      <source>min!</source>
      <cf>AVERAGE</cf>
    </def>
    <def>
      <name>min-min</name>
      <rrd>${rrdfile}</rrd>
      <source>min!</source>
      <cf>MIN</cf>
    </def>
    <def>
      <name>min-max</name>
      <rrd>${rrdfile}</rrd>
      <source>min!</source>
      <cf>MAX</cf>
    </def>
    <def>
      <name>min-last</name>
      <rrd>${rrdfile}</rrd>
      <source>min!</source>
      <cf>LAST</cf>
    </def>
  </datasources>
  <graph>

    <area>
      <datasource>min-avg</datasource>
      <color>#FFFFCC</color>
      <legend>Minimum time (ms)</legend>
    </area>
    <gprint>
      <datasource>min-avg</datasource>
      <cf>AVERAGE</cf>
      <format>Avg = %10.3f</format>
    </gprint>
    <gprint>
      <datasource>min-max</datasource>
      <cf>MAX</cf>
      <format>Max = %10.3f</format>
    </gprint>
    <gprint>
      <datasource>min-last</datasource>
      <cf>LAST</cf>
      <format>Current = %10.3f\r</format>
    </gprint>
    
    <comment>\r</comment>
    <comment>${generatedstamp}</comment>
    <comment>Performance results provided by JETM (http://jetm.void.fm)\r</comment>

  </graph>
</rrd_graph_def>